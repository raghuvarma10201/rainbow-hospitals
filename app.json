{
  "expo": {
    "name": "rainbow-hospitals",
    "slug": "rainbow-hospitals",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/br-icon.png",
    "scheme": "rainbowhospitals",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/br-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.raghuvarma1025.rainbowhospitals"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/br-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ff9cb0eb-8556-414d-bf38-bce85d84312c"
      }
    },
    "owner": "raghuvarma1025"
  }
}
